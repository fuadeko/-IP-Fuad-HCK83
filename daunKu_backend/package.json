{
  "name": "daunku_backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "dev": "node --watch app.js",
    "db:create": "cross-env NODE_ENV=development npx sequelize-cli db:create",
    "migrate": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "migrate:undo": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo:all",
    "db:seed": "cross-env NODE_ENV=development npx sequelize-cli db:seed:all",
    "db:drop": "cross-env NODE_ENV=development npx sequelize-cli db:drop",
    "db:reset": "npm run db:drop && npm run db:create && npm run migrate && npm run db:seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "form-data": "^4.0.2",
    "google-auth-library": "^9.15.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0",
    "pg": "^8.16.0",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.1"
  }
}
